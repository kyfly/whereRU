{
  "name": "Seckill",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "started": {
      "type": "date",
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    },
    "limit": {
      "type": "number",
      "default": 1
    },
    "updated": {
      "type": "date"
    },
    "title": {
      "type": "string",
      "require": true
    },
    "margin": {
      "type": "number",
      "comment": "票余量"
    }
  },
  "validations": [],
  "relations": {
    "seckillResults": {
      "type": "hasMany",
      "model": "SeckillResult"
    },
    "activity": {
      "type": "belongsTo",
      "model": "Activity"
    },
    "seckillItems": {
      "type": "embedsMany",
      "model": "SeckillItem"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "margin"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__get__seckillResults"
    }
  ],
  "methods": {}
}
