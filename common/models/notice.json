{
  "name": "Notice",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string"
    },
    "fileName": {
      "type": "string"
    },
    "uploadFile": [{
      "type": "object"
    }],
    "created": {
      "type": "date"
    },
    "updated": {
      "type": "date"
    },
    "deleted": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "form": {
      "type": "hasOne",
      "model": "Form"
    },
    "vote": {
      "type": "hasOne",
      "model": "Vote"
    },
    "seckill": {
      "type": "hasOne",
      "model": "Seckill"
    }
  },
  "acls": [],
  "methods": {},
  "scope": {
    "where": {
      "hidden": false,
      "deleted": false
    }
  }
}
