{
  "name": "Vote",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "limit": {
      "type": "number",
      "default": 1
    },
    "max": {
      "type": "number",
      "required": true
    },
    "cycle": {
      "type": "number",
      "default": 0,
      "commit": "用户可投票周期，单位小时"
    },
    "updated": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "voteResults": {
      "type": "hasMany",
      "model": "VoteResult"
    },
    "voteItems": {
      "type": "embedsMany",
      "model": "VoteItem",
      "options": {
        "validate": true
      }
    },
    "activity": {
      "type": "belongsTo",
      "model": "Activity"
    }
  },
  "acls": [],
  "methods": {}
}
