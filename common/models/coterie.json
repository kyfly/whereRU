{
  "name": "Coterie",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "logoUrl": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date"
    },
    "teamId": {
      "type": "string"
    },
    "school": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    }
  },
  "validations": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__count__fans"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__get__articles"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__findById__articles"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "relations": {
    "fans": {
      "type": "hasMany",
      "model": "CoteriewhereRUUser",
      "foreignKey": "coterieId"
    },
    "articles": {
      "type": "hasMany",
      "model": "Article"
    }
  },
  "acls": [],
  "methods": {}
}
