{
  "name": "Race",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "imgUrl": {
      "type": "string"
    },
    "authorName": {
      "type": "string"
    },
    "authorId": {
      "type": "string"
    },

    "explainUrl": {
      "type": "string"
    },
    "started": {
      "type": "date"
    },
    "school": {
      "type": "string"
    },
    "ended": {
      "type": "date"
    },
    "created": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "raceTeams": {
      "type": "hasAndBelongsToMany",
      "model": "Team"
    },
    "materials": {
      "type": "hasMany",
      "model": "Material"
    },
    "notices": {
      "type": "hasMany",
      "model": "Notice"
    },
    "team": {
      "type": "belongsTo",
      "model": "Team"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "search"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "getMySchoolRaces"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__link__raceTeams"
    }
  ],
  "methods": {}
}
