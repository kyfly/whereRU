{
  "name": "ContestOrg",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "school": {
      "type": "string"
    },
    "loader": {
      "type": "string"
    },
    "phone": {
      "type": "number"
    },
    "password": {
      "type": "string"
    },
    "authorizeId": {
      "type": "string"
    },
    "authCode": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "contests": {
      "type": "hasMany",
      "model": "Contest",
      "foreignKey": "contestOrgId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__contests"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__contests"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__destroyById__contests"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__updateById__contests"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    }
  ],
  "methods": {}
}
